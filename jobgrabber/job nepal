late_list({					list: listitem_first.find('ul'),					listitem: listitem_first.find('li'),					speed: 'fast',					mini_list:true				});			}			else			{				listitem_first.find('a.see-more').slideUp();				listitem_first.animate({					'margin-top': '-' + listitem_first.outerHeight()					}, speed, function(){						options.list.append(listitem_first);						listitem_first.css('margin-top', 0);						listitem_first.find('.on-the-way').each(function(){							$(this).removeClass('on-the-way');						});				});				}		}		else		{			listitem_first.animate({				'margin-top': '-' + listitem_first.outerHeight()				}, speed, function(){					options.list.append(listitem_first);					listitem_first.css('margin-top', 0);			});		}			}}the_interval = 0;$(function(){	the_interval = setInterval(function(){		circulate_list({			list: '#hotjobList', 			listitem: '.company-item', 			speed: speed});	}, pause_time);	$('#hotjobList .company-item').hover(function(ev){	    clearInterval(the_interval);	}, function(ev){	    the_interval = setInterval(function(){			circulate_list({				list: '#hotjobList', 				listitem: '.company-item', 				speed: speed});		}, pause_time);	});	$('a.see-more').click(function(){		list = $(this).parents('.multiple');		list.find('.on-the-way').removeClass('on-the-way');		$(this).slideUp();	});});</script>		<div id="post-loading-temp-containerjn" style="display: none;"><div id="x1-block" class="ss-small the-x1block-div" style="display:none;">			<div id="find-jobs" class="f-left">			<div id="find-jobs-content">				<form class="form" name="job-search" id="job-search" method="post" action="http://www.jobsnepal.com/simple-job-search">	<div class="find-float">		<label>Find Jobs</label>		<input type="text" name="Keywords" id="Keywords" value="eg: Web Developer" autocomplete = "off" /> 	</div>			<div class="find-float">		<label>Location</label>		<input type="text" id="JobLocation" name="JobLocation" value="eg: Lalitpur" />	</div>	<div class="find-float">		<label>Filter by Category</lab